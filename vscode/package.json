{
  "name": "pixcil",
  "displayName": "Pixcil",
  "publisher": "sile",
  "license": "MIT",
  "description": "Pixel art editor & PNG file viewer",
  "version": "0.8.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "pixel art editor",
    "png viewer"
  ],
  "browser": "./dist/web/extension.js",
  "bugs": {
    "url": "https://github.com/sile/pixcil/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sile/pixcil.git"
  },
  "homepage": "https://github.com/sile/pixcil/blob/main/README.md",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#fafcfc",
    "theme": "light"
  },
  "activationEvents": [],
  "contributes": {
    "customEditors": [
      {
        "viewType": "pixcil.png",
        "displayName": "Pixcil",
        "selector": [
          {
            "filenamePattern": "*.png"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/webpack-env": "^1.18.0",
    "process": "^0.11.10",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.0.0"
  }
}
